@model IEnumerable<MUE.Models.Message>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>

        <th>
            User
        </th>
        <th>
            Date
        </th>
        <th>
            Message
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.AspNetUser1.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DATETIMEMADE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TEXT)
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=item.ID }) 
         
        </td>
    </tr>
}

</table>
