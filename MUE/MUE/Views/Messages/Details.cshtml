@model IEnumerable<MUE.Models.Message>

@{
    ViewBag.Title = "Message";
}

<table class="table">
    <tr>

        <th>
            User
        </th>
        <th>
            Date
        </th>
        <th>
            Message
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.AspNetUser1.Email)
                <hr>
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.DATETIMEMADE)
                <hr>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TEXT)
                <hr>
            </td>
            
 
          
        </tr>
    }

</table>
